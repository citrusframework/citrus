<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="dockerClient" class="org.mockito.Mockito" factory-method="mock">
    <constructor-arg><value type="java.lang.Class">com.github.dockerjava.api.DockerClient</value></constructor-arg>
    <constructor-arg value="dockerClient"/>
  </bean>

</beans>
