<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:citrus-si="http://www.citrusframework.org/schema/spring-integration/config"
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
                            http://www.citrusframework.org/schema/spring-integration/config http://www.citrusframework.org/schema/spring-integration/config/citrus-spring-integration-config.xsd">

    <citrus-si:channel id="channel1"/>

    <citrus-si:channel id="channel2" capacity="5"/>

    <citrus-si:channel id="channel3" polling-interval="550"/>

    <citrus-si:message-channel id="channel4"/>

    <citrus-si:message-channel id="channel5" capacity="5"/>

    <citrus-si:message-channel id="channel6" polling-interval="550"/>
</beans>
